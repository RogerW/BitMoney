!!!
%html{lang: "ru", itemscope:true, itemtype:"http://schema.org/Product"}
  %head
    %meta{'http-equiv'=>"X-UA-Compatible", content:"IE=edge,chrome=1"}
    %meta{:name => "viewport", :content => "width=device-width, minimum-scale=1.0, initial-scale=1.0, user-scalable=yes", :charset => "utf-8"}
    %title= content_for?(:title) ? yield(:title) : 'MoneyBit.me'
    %meta{:name => "description", :content => "#{content_for?(:description) ? yield(:description) : 'MoneyBit'}"}
    %meta{itemprop:"brand", content:"MoneyBit"}
    = google_webfonts_init({google: [ 'Kaushan Script']})

    = stylesheet_link_tag "application", media: "all"
    = javascript_include_tag "application"
    -#= html_import_tag 'application'
    %link{rel:"import", href:"/assets/polymer/polymer.html"}
    %link{rel:"import", href:"/assets/core-media-query/core-media-query.html"}
    %link{rel:"import", href:"/assets/core-icons/core-icons.html"}
    %link{rel:"import", href:"/assets/core-icon/core-icon.html"}
    %link{rel:"import", href:"/assets/core-icon-button/core-icon-button.html"}
    %link{rel:"import", href:"/assets/core-menu-button/core-menu-button.html"}
    %link{rel:"import", href:"/assets/core-dropdown/core-dropdown.html"}
    %link{rel:"import", href:"/assets/core-item/core-item.html"}
    %link{rel:"import", href:"/assets/core-collapse/core-collapse.html"}
    %link{rel:"import", href:"/assets/app-header/app-header.html"}
    %link{rel:"import", href:"/assets/app-bar/app-bar.html"}
    %link{rel:"import", href:"/assets/paper-button/paper-button.html"}
    %link{rel:"import", href:"/assets/paper-item/paper-item.html"}
    %link{rel:"import", href:"/assets/paper-toggle-button/paper-toggle-button.html"}
    %link{rel:"import", href:"/assets/paper-fab/paper-fab.html"}
    %link{rel:"import", href:"/assets/app-signin/app-signin.html"}
    %link{rel:"import", href:"/assets/app-grid/app-grid.html"}
    %link{rel:"import", href:"/assets/app-card/app-card.html"}
    %link{rel:"import", href:"/assets/app-transfers/app-transfers.html"}
    %link{rel:"import", href:"/assets/paper-input/paper-input.html"}
    %link{rel:"import", href:"/assets/core-ajax/core-ajax.html"}
    %link{rel:"import", href:"/assets/core-scaffold/core-scaffold.html"}
    %link{rel:"import", href:"/assets/core-header-panel/core-header-panel.html"}
    %link{rel:"import", href:"/assets/core-toolbar/core-toolbar.html"}
    %link{rel:"import", href:"/assets/core-collapse/core-collapse.html"}
    %link{rel:"import", href:"/assets/core-selector/core-selector.html"}
    = csrf_meta_tags

  %body{unresolved:true}
    = render "layouts/navigation"

    = yield

    -#= javascript_include_tag "body"
